      type: application/javascript
      script: >
        var heatGroup, scheduleKind, weekDay, scheduleOfDay, scheduleAsObject,
        timesCount, i, object, startTime, scheduleStart, endTime, scheduleEnd,
        modeTime, posTime;



        //group of the heating-items

        heatGroup = ctx['group'];

        //kind of schedule being edited

        scheduleKind = ctx['whichSched'];

        //weekday being edited

        weekDay = ctx['dayOfWeek'];

        //mode,start,end from schedule for this day

        scheduleOfDay = ctx['timesArray'];

        scheduleAsObject = scheduleOfDay;

        //number of time pairs

        timesCount = scheduleAsObject.length;

        items.getItem((String(heatGroup) +
        '_art')).sendCommand(([scheduleKind,':',weekDay].join('')));

        for (i = 0; i <= 3; i++) {
          if (i <= timesCount - 1) {
            //set used time pairs to today,start- end-time,mode
            object = scheduleAsObject[(String(i))];
            scheduleStart = object['start'].split(':');
            startTime = time.ZonedDateTime.now().withYear(((time.ZonedDateTime.now()).year())).withMonth(((time.ZonedDateTime.now()).monthValue())).withDayOfMonth(((time.ZonedDateTime.now()).dayOfMonth())).withHour(parseFloat(scheduleStart['0'])).withMinute(parseFloat(scheduleStart['1'])).withSecond(0).withNano(0);
            scheduleEnd = object['end'].split(':');
            endTime = time.ZonedDateTime.now().withYear(((time.ZonedDateTime.now()).year())).withMonth(((time.ZonedDateTime.now()).monthValue())).withDayOfMonth(((time.ZonedDateTime.now()).dayOfMonth())).withHour(parseFloat(scheduleEnd['0'])).withMinute(parseFloat(scheduleEnd['1'])).withSecond(0).withNano(0);
            modeTime = object['mode'];
            posTime = object['position'];
            console.info((['ObjectNr: ',String(i),' - Start: ',startTime,' - End: ',endTime,' - Mode: ',modeTime,' - Pos: ',posTime].join('')));
            items.getItem(([heatGroup,'_ts',i + 1].join(''))).sendCommand(startTime);
            items.getItem(([heatGroup,'_te',i + 1].join(''))).sendCommand(endTime);
            items.getItem(([heatGroup,'_m',i + 1].join(''))).sendCommand(modeTime);
            items.getItem(([heatGroup,'_p',i + 1].join(''))).sendCommand(parseFloat(posTime));
          } else {
            //set unused time pairs to reduced, today at 00:00:00
            startTime = time.ZonedDateTime.now().withYear(((time.ZonedDateTime.now()).year())).withMonth(((time.ZonedDateTime.now()).monthValue())).withDayOfMonth(((time.ZonedDateTime.now()).dayOfMonth())).withHour(0).withMinute(0).withSecond(0).withNano(0);
            endTime = time.ZonedDateTime.now().withYear(((time.ZonedDateTime.now()).year())).withMonth(((time.ZonedDateTime.now()).monthValue())).withDayOfMonth(((time.ZonedDateTime.now()).dayOfMonth())).withHour(0).withMinute(0).withSecond(0).withNano(0);
            items.getItem(([heatGroup,'_ts',i + 1].join(''))).sendCommand(startTime);
            items.getItem(([heatGroup,'_te',i + 1].join(''))).sendCommand(endTime);
            items.getItem(([heatGroup,'_m',i + 1].join(''))).sendCommand('reduced');
            items.getItem(([heatGroup,'_p',i + 1].join(''))).sendCommand(0);
          }
        }
    type: script.ScriptAction
