      type: application/javascript
      script: >
        var tNr, zP1, zP2, ZpNr, anzahlZeitpaare, day, newSchedule, kind, x,
        heatGroup, tmpSchedule, buildSchedule, countZP, workMode, scheduleKind,
        days, weekDay, i, schedule, action, zeitPaar1, k, posTmp, timeNr,
        daySchedule, pos, timesCount, timeNrTmp, counter, newDaySchedule, j,
        finalSchedule, zeitPaar2, zeitPaarTmp;


        // graalVM

        function zdtCompare(zdt1, zdt2, compareOp, precision, compDate) {
          switch (precision) {
            case 'years':
             zdt2 = zdt2.withMonth(zdt1.monthValue());
            case 'months':
             zdt2 = zdt2.withDayOfMonth(zdt1.dayOfMonth());
            case 'days':
             zdt2 = zdt2.withHour(zdt1.hour());
            case 'hours':
             zdt2 = zdt2.withMinute(zdt1.minute());
            case 'minutes':
             zdt2 = zdt2.withSecond(zdt1.second());
            case 'seconds':
             zdt2 = zdt2.withNano(zdt1.nano());
          }
          if (compDate === 'date') {
            zdt1 = zdt1.toLocalDate();
            zdt2 = zdt2.toLocalDate();
          } else if (compDate === 'time') {
            zdt1 = zdt1.toLocalTime();
            zdt2 = zdt2.toLocalTime();
          }
          switch (compareOp) {
            case 'before':
              return zdt1.isBefore(zdt2);
            case 'equal':
              return zdt1.equals(zdt2);
            case 'after':
              return zdt1.isAfter(zdt2);
            case 'beforeEqual':
              return zdt1.isBefore(zdt2) || zdt1.equals(zdt2);
            case 'afterEqual':
              return zdt1.isAfter(zdt2) || zdt1.equals(zdt2);
          }
        }


        // Describe this function...

        function updateProxyItems(tNr, zP1, zP2) {
          //Tausche die beiden Zeitpaare
          items.getItem(([heatGroup,'_ts',tNr - 1].join(''))).sendCommand(zP1['startZeit']);
          items.getItem(([heatGroup,'_te',tNr - 1].join(''))).sendCommand(zP1['endZeit']);
          items.getItem(([heatGroup,'_m',tNr - 1].join(''))).sendCommand(zP1['mode']);
          items.getItem(([heatGroup,'_p',tNr - 1].join(''))).sendCommand(zP1['pos']);
          items.getItem(([heatGroup,'_ts',tNr].join(''))).sendCommand(zP2['startZeit']);
          items.getItem(([heatGroup,'_te',tNr].join(''))).sendCommand(zP2['endZeit']);
          items.getItem(([heatGroup,'_m',tNr].join(''))).sendCommand(zP2['mode']);
          items.getItem(([heatGroup,'_p',tNr].join(''))).sendCommand(zP2['pos']);
        }


        // Baut des Schedule des 체bergebenen Tages

        function buildDaySchedule(ZpNr, anzahlZeitpaare) {
          //Erstellt Schedule des Tages bei delete & update
          countZP = 0;
          tmpSchedule = ['"',weekDay,'":'].join('');
          tmpSchedule += '[';
          var i_inc = 1;
          if (1 > anzahlZeitpaare) {
            i_inc = -i_inc;
          }
          for (i = 1; i_inc >= 0 ? i <= anzahlZeitpaare : i >= anzahlZeitpaare; i += i_inc) {
            if (i != ZpNr) {
              zeitPaar1 = {'startZeit': items.getItem(([heatGroup,'_ts',i].join(''))).state, 'endZeit': items.getItem(([heatGroup,'_te',i].join(''))).state, 'mode': items.getItem(([heatGroup,'_m',i].join(''))).state, 'pos': items.getItem(([heatGroup,'_p',i].join(''))).state};
              posTmp = (parseFloat(zeitPaar1['pos'])).toFixed(0);
              if (posTmp > countZP) {
                pos = posTmp - 1;
                countZP = countZP + 1;
              } else {
                pos = posTmp;
                countZP = countZP + 1;
              }
              tmpSchedule += '{';
              tmpSchedule += String(['"start":"',(time.toZDT(zeitPaar1['startZeit'])).format(time.DateTimeFormatter.ofPattern('HH:mm')),'","end":"',(time.toZDT(zeitPaar1['endZeit'])).format(time.DateTimeFormatter.ofPattern('HH:mm')),'","mode":"',zeitPaar1['mode'],'","position":',pos].join(''));
              tmpSchedule += '}';
              if (i < anzahlZeitpaare) {
                tmpSchedule += ',';
              }
            }
          }
          tmpSchedule += ']';
          return tmpSchedule;
        }


        // Describe this function...

        function buildSchedule2(day, newSchedule, kind) {
          console.info('Start building new Schedule.');
          if (kind == 'HZ') {
            //lese das Schedule f체r die Heizung
            schedule = (JSON.parse(items.getItem((String(heatGroup) + '_Heating_schedule_Circuit_1')).state)
            );
          } else if (kind == 'WW') {
            //lese das Schedule f체r das Warmwasser
            schedule = (JSON.parse(items.getItem((String(heatGroup) + '_DHW_schedule')).state)
            );
          } else {
            //lese das Schedule f체r die Zirkulation
            schedule = (JSON.parse(items.getItem((String(heatGroup) + '_DHW_circulation_pump__schedule')).state)
            );
          }
          days = 'mon,tue,wed,thu,fri,sat,sun'.split(',');
          buildSchedule = '{';
          for (var k_index in days) {
            k = days[k_index];
            if (k != day) {
              daySchedule = schedule[k];
              buildSchedule += String(['"',k,'":',JSON.stringify(daySchedule)
              ].join(''));
              if (k != 'sun') {
                buildSchedule += ',';
              }
            } else {
              buildSchedule += String(newSchedule);
              if (day != 'sun') {
                buildSchedule += ',';
              }
            }
          }
          buildSchedule += '}';
          return buildSchedule;
        }


        // Describe this function...

        function updateSchedule(x) {
          console.info((['Update Schedule: ',scheduleKind,'\r\n','mit : ',x].join('')));
          if (scheduleKind == 'HZ') {
            //update das Schedule f체r die Heizung
            items.getItem((String(heatGroup) + '_Heating_schedule_Circuit_1')).sendCommand(x);
          } else if (scheduleKind == 'WW') {
            //update das Schedule f체r das Warmwasser
            items.getItem((String(heatGroup) + '_DHW_schedule')).sendCommand(x);
          } else {
            //update das Schedule f체r die Zirkulation
            items.getItem((String(heatGroup) + '_DHW_circulation_pump__schedule')).sendCommand(x);
          }
        }



        //Die Gruppe der Heizungsitems

        heatGroup = ctx['group'];

        //Schedule-Art und Tag in Bearbeitung

        workMode = items.getItem((String(heatGroup) + '_art')).state.split(':');

        scheduleKind = workMode['0'];

        weekDay = workMode['1'];

        //Aufgabe die ansteht

        action = ctx['action'];

        //Nummer des Zeitpaars kann > timesCount sein

        timeNr = ctx['timeNr'];

        //Anzahl der Zeitpaare im Schedule vor Update

        timesCount = ctx['timesCount'];

        if (action == 'delete') {
          //Zeitpaar entfernen, danach schedule updaten
          timeNrTmp = timeNr;
          newDaySchedule = buildDaySchedule(timeNrTmp, timesCount);
          finalSchedule = buildSchedule2(weekDay, newDaySchedule, scheduleKind);
          updateSchedule(finalSchedule);
        } else if (action == 'save' && timeNr <= timesCount) {
          //Anzahl Zeitpaare unver채ndert, Schedule nur updaten
          timeNrTmp = 0;
          newDaySchedule = buildDaySchedule(timeNrTmp, timesCount);
          finalSchedule = buildSchedule2(weekDay, newDaySchedule, scheduleKind);
          updateSchedule(finalSchedule);
        } else {
          //Zeitpaare hinzugef체gt, ins Schedule einf체gen
          counter = timeNr;
          while (counter > 0) {
            zeitPaar1 = {'startZeit': items.getItem(([heatGroup,'_ts',counter].join(''))).state, 'endZeit': items.getItem(([heatGroup,'_te',counter].join(''))).state, 'mode': items.getItem(([heatGroup,'_m',counter].join(''))).state, 'pos': items.getItem(([heatGroup,'_p',counter].join(''))).state};
            //Mehr als 2 Zeitpaare, pr체fen auf Tausch
            if (counter >= 2) {
              zeitPaar2 = {'startZeit': items.getItem(([heatGroup,'_ts',counter - 1].join(''))).state, 'endZeit': items.getItem(([heatGroup,'_te',counter - 1].join(''))).state, 'mode': items.getItem(([heatGroup,'_m',counter - 1].join(''))).state, 'pos': items.getItem(([heatGroup,'_p',counter - 1].join(''))).state};
              if (zdtCompare((time.toZDT(zeitPaar1['endZeit'])), (time.toZDT(zeitPaar2['startZeit'])), 'before', 'minutes', 'dateandtime')) {
                zeitPaarTmp = zeitPaar1;
                updateProxyItems(counter, zeitPaar1, zeitPaar2);
              }
            }
            counter = counter - 1;
          }
          var j_end = timesCount + 1;
          var j_inc = 1;
          if (1 > j_end) {
            j_inc = -j_inc;
          }
          for (j = 1; j_inc >= 0 ? j <= j_end : j >= j_end; j += j_inc) {
            items.getItem(([heatGroup,'_p',j].join(''))).sendCommand(j);
          }
          timeNrTmp = 0;
          newDaySchedule = buildDaySchedule(timeNrTmp, timesCount + 1);
          finalSchedule = buildSchedule2(weekDay, newDaySchedule, scheduleKind);
          updateSchedule(finalSchedule);
        }
    type: script.ScriptAction
